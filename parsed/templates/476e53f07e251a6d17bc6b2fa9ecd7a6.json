{
  "components/category/shop-by-price": "{{#and theme_settings.shop_by_price_visibility shop_by_price}}\n    <div class=\"sidebarBlock\" id=\"shopByPrices\">\n        <h3 class=\"sidebarBlock-heading\">{{lang 'category.shop_by_price'}}</h3>\n        <ul class=\"navList\">\n            {{#each shop_by_price}}\n            <li class=\"navList-item\">\n                <a \n                    {{#if selected }} \n                        class=\"navList-action is-active\"\n                        role=\"status\"\n                        aria-live=\"polite\"\n                   {{else}}\n                        class=\"navList-action\"\n                   {{/if}}\n                   href=\"{{url}}\"\n                   title=\"{{low.formatted}} - {{high.formatted}}\"\n                   aria-label=\"{{{lang 'category.shop_by_price_range_aria' from=low.formatted to=high.formatted}}}\"\n                >\n                    <span class=\"text\">{{low.formatted}} - {{high.formatted}}</span>\n                </a>\n            </li>\n            {{/each}}\n\n            {{#any shop_by_price selected=true}}\n                <li class=\"navList-item navList-reset\">\n                    <a href=\"{{category_url}}\" class=\"navList-action reset-btn\">\n                        <span class=\"text\">{{lang 'category.reset'}}</span>\n                    </a>\n                    <span class=\"reset-message aria-description--hidden\">{{lang 'category.filter_reset_announcement'}}</span>\n                </li>\n            {{/any}}\n        </ul>\n    </div>\n{{/and}}\n"
}