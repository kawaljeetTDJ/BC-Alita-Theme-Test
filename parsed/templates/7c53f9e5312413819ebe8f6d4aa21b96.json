" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-7)}}&section=product\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-7}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.halo-before-quick-results-trending-now-8}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-8)}}&section=product\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-8}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.halo-before-quick-results-trending-now-9}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-9)}}&section=product\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-9}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n                {{#if theme_settings.halo-before-quick-results-trending-now-10}}\r\n                    <li class=\"search-item\">\r\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-10)}}&section=product\">\r\n                            <svg class=\"icon\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-10}}\r\n                        </a>\r\n                    </li>\r\n                {{/if}}\r\n            </ul>\r\n        </div>\r\n    {{/if}}\r\n    {{#if theme_settings.halo-before-quick-results-popular}}\r\n        <div class=\"quickResults-item quickResults-product\">\r\n            {{#if theme_settings.halo-before-quick-results-popular-title}}\r\n                <h3 class=\"quickResults-title\">{{theme_settings.halo-before-quick-results-popular-title}}</h3>\r\n            {{/if}}\r\n            <div class=\"loadingOverlay\"></div>\r\n            <ul class=\"productGrid haloCustomScrollbar-x\"></ul>\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n"
