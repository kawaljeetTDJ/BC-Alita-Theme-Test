{
  "components/halothemes/popup/halo-recently-viewed-popup": "{{#if theme_settings.halo_recently_viewed_products}}\r\n    <div class=\"halo-popup halo-recently-viewed-popup\" id=\"halo-recently-viewed-popup\">\r\n        <div class=\"halo-recently-viewed halo-recently-viewed-list\" id=\"halo-recently-viewed-list\">\r\n            <div class=\"halo-recently-viewed-collapse\" data-collapse-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"Collapse\">\r\n                    <title>Collapse</title>\r\n                    <desc>Collapse</desc>\r\n                    <use xlink:href=\"#icon-collapse\"></use>\r\n                </svg>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-top\">\r\n                <div class=\"cart-icon with-products\">\r\n                    <a href=\"{{urls.cart}}\" data-cart-preview>\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.cart_preview'}}\">\r\n                            <title>{{lang 'cart.cart_preview'}}</title>\r\n                            <desc>{{lang 'cart.cart_preview'}}</desc>\r\n                            <use xlink:href=\"#icon-cart\"></use>\r\n                        </svg>\r\n                        <span class=\"cart-quantity {{#if cart.items}} countPill--positive{{/if}}\">{{cart.quantity}}</span>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-content\">\r\n                {{#if theme_settings.halo_recently_viewed_products_title}}\r\n                    <h3 class=\"title\">{{theme_settings.halo_recently_viewed_products_title}}</h3>\r\n                {{/if}}\r\n                <div class=\"products-grid\" id=\"recently-viewed-products-list\">\r\n                    <div class=\"no-products\" style=\"flex-direction: column; display:none;\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.no_item'}}\">\r\n                            <title>{{lang 'common.no_item'}}</title>\r\n                            <desc>{{lang 'common.no_item'}}</desc>\r\n                            <use xlink:href=\"#icon-no-products\"></use>\r\n                        </svg>\r\n                        <div class=\"text\">{{lang 'common.no_item'}}</div>\r\n                    </div>\r\n                </div>\r\n                <div id=\"halo-recently-viewed-tmp\" style=\"display: none!important;\"></div>\r\n            </div>\r\n            {{#if theme_settings.halo_recently_viewed_products_top }}\r\n                <div class=\"halo-recently-viewed-bottom\" data-scroll-top-popup>\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.top'}}\">\r\n                        <title>{{lang 'common.top'}}</title>\r\n                        <desc>{{lang 'common.top'}}</desc>\r\n                        <use xlink:href=\"#icon-cross-line-top\"></use>\r\n                    </svg>\r\n                    <div class=\"text\">{{lang 'common.top'}}</div>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n        <div class=\"halo-recently-viewed halo-recently-viewed-wrap\" id=\"halo-recently-viewed-wrap\">\r\n            <div class=\"halo-recently-viewed-icon\" data-expand-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"Expand\">\r\n                    <title>Expand</title>\r\n                    <desc>Expand</desc>\r\n                    <use xlink:href=\"#icon-expand\"></use>\r\n                </svg>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-icon\" data-scroll-top-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.top'}}\">\r\n                    <title>{{lang 'common.top'}}</title>\r\n                    <desc>{{lang 'common.top'}}</desc>\r\n                    <use xlink:href=\"#icon-cross-line-top\"></use>\r\n                </svg>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n"
}