{
  "components/halothemes/configure-options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\">\r\n    <label class=\"form-label form-label--inlineSmall\">\r\n        {{ this.display_name }}\r\n        {{> components/common/requireness-msg}}\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio\"\r\n            type=\"radio\"\r\n            id=\"edit_attribute_radio_{{../id}}_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}\r\n                checked\r\n                data-default\r\n            {{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"edit_attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\r\n    {{/each}}\r\n</div>\r\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}*{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n"
}