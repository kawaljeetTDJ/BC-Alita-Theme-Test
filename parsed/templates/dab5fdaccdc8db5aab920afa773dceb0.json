{
  "components/halothemes/popup/halo-popup": "{{> components/halothemes/popup/halo-recently-viewed-popup}}\n\n{{> components/halothemes/popup/halo-recently-bought-popup}}\n\n{{> components/halothemes/popup/halo-newsletter-popup}}\n",
  "components/halothemes/popup/halo-recently-viewed-popup": "{{#if theme_settings.halo_recently_viewed_products}}\r\n    <div class=\"halo-popup halo-recently-viewed-popup\" id=\"halo-recently-viewed-popup\">\r\n        <div class=\"halo-recently-viewed halo-recently-viewed-list\" id=\"halo-recently-viewed-list\">\r\n            <div class=\"halo-recently-viewed-collapse\" data-collapse-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"Collapse\">\r\n                    <title>Collapse</title>\r\n                    <desc>Collapse</desc>\r\n                    <use xlink:href=\"#icon-collapse\"></use>\r\n                </svg>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-top\">\r\n                <div class=\"cart-icon with-products\">\r\n                    <a href=\"{{urls.cart}}\" data-cart-preview>\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.cart_preview'}}\">\r\n                            <title>{{lang 'cart.cart_preview'}}</title>\r\n                            <desc>{{lang 'cart.cart_preview'}}</desc>\r\n                            <use xlink:href=\"#icon-cart\"></use>\r\n                        </svg>\r\n                        <span class=\"cart-quantity {{#if cart.items}} countPill--positive{{/if}}\">{{cart.quantity}}</span>\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-content\">\r\n                {{#if theme_settings.halo_recently_viewed_products_title}}\r\n                    <h3 class=\"title\">{{theme_settings.halo_recently_viewed_products_title}}</h3>\r\n                {{/if}}\r\n                <div class=\"products-grid\" id=\"recently-viewed-products-list\">\r\n                    <div class=\"no-products\" style=\"flex-direction: column; display:none;\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.no_item'}}\">\r\n                            <title>{{lang 'common.no_item'}}</title>\r\n                            <desc>{{lang 'common.no_item'}}</desc>\r\n                            <use xlink:href=\"#icon-no-products\"></use>\r\n                        </svg>\r\n                        <div class=\"text\">{{lang 'common.no_item'}}</div>\r\n                    </div>\r\n                </div>\r\n                <div id=\"halo-recently-viewed-tmp\" style=\"display: none!important;\"></div>\r\n            </div>\r\n            {{#if theme_settings.halo_recently_viewed_products_top }}\r\n                <div class=\"halo-recently-viewed-bottom\" data-scroll-top-popup>\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.top'}}\">\r\n                        <title>{{lang 'common.top'}}</title>\r\n                        <desc>{{lang 'common.top'}}</desc>\r\n                        <use xlink:href=\"#icon-cross-line-top\"></use>\r\n                    </svg>\r\n                    <div class=\"text\">{{lang 'common.top'}}</div>\r\n                </div>\r\n            {{/if}}\r\n        </div>\r\n        <div class=\"halo-recently-viewed halo-recently-viewed-wrap\" id=\"halo-recently-viewed-wrap\">\r\n            <div class=\"halo-recently-viewed-icon\" data-expand-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"Expand\">\r\n                    <title>Expand</title>\r\n                    <desc>Expand</desc>\r\n                    <use xlink:href=\"#icon-expand\"></use>\r\n                </svg>\r\n            </div>\r\n            <div class=\"halo-recently-viewed-icon\" data-scroll-top-popup>\r\n                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.top'}}\">\r\n                    <title>{{lang 'common.top'}}</title>\r\n                    <desc>{{lang 'common.top'}}</desc>\r\n                    <use xlink:href=\"#icon-cross-line-top\"></use>\r\n                </svg>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n",
  "components/halothemes/popup/halo-recently-bought-popup": "{{#if theme_settings.haloRecentlyBought}}\r\n\t<div id=\"halo-recently-bought-popup\" class=\"halo-popup halo-recently-bought-popup\">\r\n\t\t<div class=\"hide halo-recently-bought-list\" id=\"halo-recently-bought-list\"></div>\r\n\t</div>\r\n{{/if}}\n",
  "components/halothemes/popup/halo-newsletter-popup": "{{#if theme_settings.halo_newsletter_popup}}\r\n    <div id=\"halo-newsletter-popup\" class=\"halo-popup halo-newsletter-popup hide\">\r\n        <div class=\"newsletter-popup-container\">\r\n            <div class=\"newsletter-popup-inner\">\r\n                <button type=\"button\" data-close-newsletter-popup>\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n                        <title>{{lang 'common.close'}}</title>\r\n                        <desc>{{lang 'common.close'}}</desc>\r\n                        <use xlink:href=\"#icon-close\"></use>\r\n                    </svg>\r\n                </button>\r\n                <div class=\"newsletter-left\">\r\n                    <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo_newsletter_popup_image}}\" alt=\"{{lang 'newsletter.subscribe'}}\" title=\"{{lang 'newsletter.subscribe'}}\">\r\n                </div>\r\n                <div class=\"newsletter-right\">\r\n                    <h2 class=\"newsletter-heading\">{{lang 'newsletter.subscribe'}}</h2>\r\n                    <div class=\"newsletter-message\">\r\n                        <span>{{lang 'newsletter.subscribe_intro'}}</span>\r\n                        <span>{{lang 'newsletter.subscribe_intro2'}}</span>\r\n                    </div>\r\n                    <div id=\"popupSubcribeForm\">\r\n                        <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupSubcribeFormSubmit\">\r\n                            <fieldset class=\"form-fieldset\">\r\n                                <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                                <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                                <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                                <div class=\"form-field\">\r\n                                    <label class=\"form-label is-srOnly\" for=\"nl_email2\">{{lang 'newsletter.email_placeholder'}}</label>\r\n                                    <input class=\"form-input\" id=\"nl_email2\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                                    <input class=\"button button--primary button--large\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                                </div>\r\n                            </fieldset>\r\n                        </form>\r\n                    </div>\r\n                    {{#if theme_settings.halo_newsletter_popup_again}}\r\n                        <div class=\"form-show-again\">\r\n                            <form class=\"form\">\r\n                                <div class=\"form-field form-field-checkbox\">\r\n                                    <input id=\"do-not-show-again\" class=\"do-not-show-again\" type=\"checkbox\" name=\"don't show again\" value=\"1\"> \r\n                                    <label class=\"form-label\" for=\"do-not-show-again\">\r\n                                        <span>{{lang 'newsletter.subscribe_dontshow'}}</span>\r\n                                    </label>\r\n                                </div>\r\n                            </form>\r\n                        </div>\r\n                    {{/if}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n"
}