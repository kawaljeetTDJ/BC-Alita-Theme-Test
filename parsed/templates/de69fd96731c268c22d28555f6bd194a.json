{
  "components/common/header": "{{#and (if page_type '==' 'default') (if theme_settings.haloTransparentHeader)}}\n    <header class=\"header header-transparent{{#if theme_settings.haloStickyHeader}} header-sticky{{/if}}\" role=\"banner\">\n{{else}}\n    <header class=\"header{{#if theme_settings.haloStickyHeader}} header-sticky{{/if}}\" role=\"banner\">\n{{/and}}\n    {{#if theme_settings.halo_top_bar_promotion}}\n        <div class=\"halo-top-bar-promotion\">\n            <div class=\"container\">\n                <p class=\"text\">\n                    {{theme_settings.halo-top_bar_promotion-text}}\n                </p>\n            </div>\n        </div>\n    {{/if}}\n    <div class=\"halo-topHeader\">\n        <div class=\"container\">\n            {{#or (if theme_settings.halo-book-an-appointment) (if currency_selector.currencies.length '>' 1)}}\n                <div class=\"topHeader-item text-left\">\n                    {{#if currency_selector.currencies.length '>' 1}}\n                        <div class=\"topHeader-item__item\">\n                            {{> components/common/currency-selector}}\n                        </div>\n                    {{/if}}\n                    {{#if theme_settings.halo-book-an-appointment}}\n                        <div class=\"topHeader-item__item\">\n                            <a href=\"#\" class=\"topHeader-item__link\" data-book-an-appointment>\n                                <span class=\"text\">\n                                    <span>{{theme_settings.halo-book-an-appointment-title}}</span>\n                                </span>\n                            </a>\n                        </div>\n                    {{/if}}\n                </div>\n            {{/or}}\n            <div class=\"topHeader-item text-right\">\n                {{#if customer}}\n                    <div class=\"topHeader-item__item\">\n                        <span class=\"topHeader-item__link\">\n                            <span class=\"text\">Hi, <strong>{{ellipsis customer.name 30}}</strong></span>\n                        </span>\n                    </div>\n                    <div class=\"topHeader-item__item\">\n                        <a href=\"{{urls.account.index}}\" class=\"topHeader-item__link\" data-login-form>\n                            <span class=\"text\">{{lang 'common.account'}}</span>\n                        </a>\n                    </div>\n                {{else}}\n                    <div class=\"topHeader-item__item\">\n                        <a href=\"{{urls.auth.login}}\" class=\"topHeader-item__link\" data-login-form>\n                            <span class=\"text\">\n                                <span>{{lang 'common.login'}}</span>\n                            </span>\n                        </a>\n                    </div>\n                    <div class=\"topHeader-item__item\">\n                        <a href=\"{{urls.auth.create_account}}\" class=\"topHeader-item__link\">\n                            <span class=\"text\">\n                                <span>{{lang 'common.sign_up'}}</span>\n                            </span>\n                        </a>\n                    </div>\n                {{/if}}\n            </div>\n        </div>\n    </div>\n    <div class=\"halo-middleHeader{{#unless settings.store_logo.image}} halo-middleHeader-2{{/unless}}\">\n        <div class=\"container\">\n            <div class=\"middleHeader-item text-left\">\n                <div class=\"middleHeader-item__item item--hamburger\">\n                    <a class=\"mobileMenu-toggle\" href=\"#\" data-mobile-menu-toggle=\"menu\">\n                        <span class=\"mobileMenu-toggleIcon\">{{lang 'common.toggle_menu'}}</span>\n                    </a>\n                </div>\n                <div class=\"middleHeader-item__item item--searchMobile\">\n                    <a class=\"navUser-action\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\">\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'search.quick_search.input_label'}}\">\n                                <title>{{lang 'search.quick_search.input_label'}}</title>\n                                <desc>{{lang 'search.quick_search.input_placeholder'}}</desc>\n                                <use xlink:href=\"#icon-search\"></use>\n                            </svg>\n                        </div>\n                    </a>\n                </div>\n            </div>\n            <div class=\"middleHeader-item text-center\">\n                <div class=\"middleHeader-item__item item--logo\">\n                    {{#if page_type '===' 'default'}}\n                        <h1 class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </h1>\n                    {{else}}\n                        <div class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </div>\n                    {{/if}}\n                </div>\n            </div>\n            <div class=\"middleHeader-item text-right\">\n                <div class=\"middleHeader-item__item item--quicksearch halo-search\">\n                    <a class=\"navUser-action\" href=\"#\" data-search-popup>\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'search.quick_search.input_label'}}\">\n                                <title>{{lang 'search.quick_search.input_label'}}</title>\n                                <desc>{{lang 'search.quick_search.input_placeholder'}}</desc>\n                                <use xlink:href=\"#icon-search\"></use>\n                            </svg>\n                        </div>\n                    </a>\n                </div>\n                <div class=\"middleHeader-item__item item--account\">\n                    <a class=\"navUser-action myAccountMobile\" {{#if customer}}href=\"{{urls.account.index}}\"{{else}}href=\"{{urls.auth.login}}\"{{/if}} data-login-form>\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\n                                <title>{{lang 'common.account'}}</title>\n                                <desc>{{lang 'common.account'}}</desc>\n                                <use xlink:href=\"#icon-user\"></use>\n                            </svg>\n                        </div>\n                    </a>\n                </div>\n                {{#if settings.show_wishlist}}\n                    <div class=\"middleHeader-item__item item--wishlist\">\n                        <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\">\n                            <div class=\"navUser-icon\">\n                                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                                    <title>{{lang 'account.wishlists.heading'}}</title>\n                                    <desc>{{lang 'account.wishlists.heading'}}</desc>\n                                    <use xlink:href=\"#icon-heart\"></use>\n                                </svg>\n                            </div>\n                        </a>\n                    </div>\n                {{/if}}\n                <div class=\"middleHeader-item__item item--cart halo-cart\">\n                    <a class=\"navUser-action\" data-cart-preview href=\"{{urls.cart}}\">\n                        <div class=\"navUser-icon navUser-item-cartIcon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.cart_preview'}}\">\n                                <title>{{lang 'cart.cart_preview'}}</title>\n                                <desc>{{lang 'cart.cart_preview'}}</desc>\n                                <use xlink:href=\"#icon-cart\"></use>\n                            </svg>\n                            <span class=\"countPill cart-quantity\">{{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}</span>\n                        </div>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n    <div class=\"halo-bottomHeader\">\n        <div class=\"container\">\n            <div class=\"bottomHeader-item text-left\">\n                <div class=\"bottomHeader-item__item item--sticky-logo\">\n                    {{#if page_type '===' 'default'}}\n                        <h1 class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </h1>\n                    {{else}}\n                        <div class=\"header-logo\">\n                            {{> components/common/store-logo}}\n                        </div>\n                    {{/if}}\n                </div>\n            </div>\n            <div class=\"bottomHeader-item text-center\">\n                <div class=\"navPages-container bottomHeader-item__item\" id=\"menu\" data-menu>\n                    {{> components/common/navigation-menu}}\n                </div>\n            </div>\n            <div class=\"bottomHeader-item text-right\">\n                <div class=\"bottomHeader-item__item item--quicksearch halo-search\">\n                    <a class=\"navUser-action\" href=\"#\" data-search-popup>\n                        <div class=\"navUser-icon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'search.quick_search.input_label'}}\">\n                                <title>{{lang 'search.quick_search.input_label'}}</title>\n                                <desc>{{lang 'search.quick_search.input_placeholder'}}</desc>\n                                <use xlink:href=\"#icon-search\"></use>\n                            </svg>\n                        </div>\n                    </a>\n                </div>\n                {{#if settings.show_wishlist}}\n                    <div class=\"bottomHeader-item__item item--wishlist\">\n                        <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\">\n                            <div class=\"navUser-icon\">\n                                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                                    <title>{{lang 'account.wishlists.heading'}}</title>\n                                    <desc>{{lang 'account.wishlists.heading'}}</desc>\n                                    <use xlink:href=\"#icon-heart\"></use>\n                                </svg>\n                            </div>\n                        </a>\n                    </div>\n                {{/if}}\n                <div class=\"bottomHeader-item__item item--cart halo-cart\">\n                    <a class=\"navUser-action\" data-cart-preview href=\"{{urls.cart}}\">\n                        <div class=\"navUser-icon navUser-item-cartIcon\">\n                            <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.cart_preview'}}\">\n                                <title>{{lang 'cart.cart_preview'}}</title>\n                                <desc>{{lang 'cart.cart_preview'}}</desc>\n                                <use xlink:href=\"#icon-cart\"></use>\n                            </svg>\n                            <span class=\"countPill cart-quantity\">\n                                {{#if cart.items}}{{cart.quantity}}{{else}}0{{/if}}\n                            </span>\n                        </div>\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n</header>\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n    <a class=\"topHeader-item__link topHeader-item__currency\"\n        href=\"#\"\n        data-dropdown=\"currencySelection\"\n        aria-controls=\"currencySelection\"\n        aria-expanded=\"false\"\n    >\n        <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \n        <span class=\"text\">\n            <span>{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\n        </span>\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </a>\n    <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n        {{#each currency_selector.currencies}}\n        <li class=\"dropdown-menu-item\">\n            <a href=\"{{{switch_url}}}\"\n               data-currency-code=\"{{{code}}}\"\n               {{#if cart_currency_switch_url.length '>' 0 }}\n               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n               {{/if}}\n               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n              {{#if is_active}} class=\"is-active\"{{/if}}\n            >\n                <span class=\"flag\">{{{flag}}}</span>\n                <span class=\"text\">{{code}}</span>\n            </a>\n        </li>\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <span class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </span>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}\" {{#if theme_settings.haloMenuResize}} id=\"halo-menu-resize\"{{/if}}>\n        {{#each categories}}\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                {{> components/common/navigation-list-alternate}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                {{#if name '!=' 'Blog'}}\n                    <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                        {{> components/common/navigation-list-alternate}}\n                    </li>\n                {{/if}}\n            {{/each}}\n        {{/unless}}\n        {{#if theme_settings.haloMenuResize}}\n            <li class=\"navPages-item navPages-item-toggle u-hiddenVisually\">\n                <p class=\"navPages-action has-subMenu is-root\">\n                    <span class=\"text\">\n                        <svg><use xlink:href=\"#icon-more\"></use></svg>\n                    </span>\n                </p>\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\" id=\"halo-navPages-dropdown\" data-dropdown-menu-content></ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <p class=\"navPages-action has-subMenu is-root\" data-label=\"{{url}}\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action navPages-action-end\" data-label=\"{{url}}\" href=\"{{url}}\">\n      <span class=\"text\">{{name}}</span>\n    </a>\n{{/if}}\n\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child has-dropdown\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span>\n                    </p>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n                        <ul class=\"navPage-subMenu-list\">\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                <p class=\"navPage-subMenu-action navPages-action\">\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                                    </span>\n                                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                </p>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}