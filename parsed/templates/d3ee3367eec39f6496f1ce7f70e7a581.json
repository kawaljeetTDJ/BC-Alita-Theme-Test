<desc>{{lang 'account.nav.returns'}}</desc>\r\n\t\t                                <use xlink:href=\"#icon-return\"></use>\r\n\t\t                            </svg>\r\n\t\t\t                    \t{{lang 'account.nav.returns'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.inbox}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.mobile_nav.messages'}}\">\r\n\t                                <title>{{lang 'account.mobile_nav.messages'}}</title>\r\n\t                                <desc>{{lang 'account.mobile_nav.messages'}}</desc>\r\n\t                                <use xlink:href=\"#icon-message\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.mobile_nav.messages'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t             <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.addresses}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.addresses'}}\">\r\n\t                                <title>{{lang 'account.nav.addresses'}}</title>\r\n\t                                <desc>{{lang 'account.nav.addresses'}}</desc>\r\n\t                                <use xlink:href=\"#icon-address-book\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.addresses'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            {{#if settings.show_payment_methods}}\r\n\t\t                <li class=\"account-item\">\r\n\t\t                    <a class=\"account-action\" href=\"{{urls.account.payment_methods.all}}\">\r\n\t\t                        <span class=\"text\">\r\n\t\t                        \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.payment_methods'}}\">\r\n\t\t                                <title>{{lang 'account.nav.payment_methods'}}</title>\r\n\t\t                                <desc>{{lang 'account.nav.payment_methods'}}</desc>\r\n\t\t                                <use xlink:href=\"#icon-payment\"></use>\r\n\t\t                            </svg>\r\n\t\t\t                    \t{{lang 'account.nav.payment_methods'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            {{#if settings.show_wishlist}}\r\n\t\t                <li class=\"account-item\">\r\n\t\t                    <a class=\"account-action\" href=\"{{urls.account.wishlists.all}}\">\r\n\t\t                        <span class=\"text\">\r\n\t\t                        \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n\t                                    <title>{{lang 'account.wishlists.heading'}}</title>\r\n\t                                    <desc>{{lang 'account.wishlists.heading'}}</desc>\r\n\t                                    <use xlink:href=\"#icon-heart\"></use>\r\n\t                                </svg>\r\n\t\t                        \t{{lang 'account.mobile_nav.wishlists'}}\r\n\t\t                        </span>\r\n\t\t                    </a>\r\n\t\t                </li>\r\n\t\t            {{/if}}\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.recent_items}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.recently_viewed'}}\">\r\n\t                                <title>{{lang 'account.nav.recently_viewed'}}</title>\r\n\t                                <desc>{{lang 'account.nav.recently_viewed'}}</desc>\r\n\t                                <use xlink:href=\"#icon-recently-viewed\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.recently_viewed'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.account.details}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.nav.settings'}}\">\r\n\t                                <title>{{lang 'account.nav.settings'}}</title>\r\n\t                                <desc>{{lang 'account.nav.settings'}}</desc>\r\n\t                                <use xlink:href=\"#icon-account-settings\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'account.nav.settings'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t            <li class=\"account-item\">\r\n\t\t                <a class=\"account-action\" href=\"{{urls.auth.logout}}\">\r\n\t\t                    <span class=\"text\">\r\n\t\t                    \t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.logout'}}\">\r\n\t                                <title>{{lang 'common.logout'}}</title>\r\n\t                                <desc>{{lang 'common.logout'}}</desc>\r\n\t                                <use xlink:href=\"#icon-sign-out\"></use>\r\n\t                            </svg>\r\n\t\t                    \t{{lang 'common.logout'}}\r\n\t\t                    </span>\r\n\t\t                </a>\r\n\t\t            </li>\r\n\t\t        </ul>\r\n\t\t    </div>\r\n\t\t</div>\r\n\t{{else}}\r\n\t\t<div id=\"halo-auth-sidebar\" class=\"halo-sidebar login-form-sidebar halo-auth-sidebar halo-sidebar-right\">\r\n\t\t\t<div class=\"halo-sidebar-header\">\r\n\t\t\t\t<span class=\"title\">{{lang 'login.title'}}</span>\r\n\t\t\t\t<a href=\"#\" class=\"close\">\r\n\t\t\t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t\t\t        \t<title>{{lang 'common.close'}}</title>\r\n\t\t\t        \t<desc>{{lang 'common.close'}}</desc>\r\n\t\t\t        \t<use xlink:href=\"#icon-close\"></use>\r\n\t\t\t        </svg>\r\n\t\t\t\t</a>\r\n\t\t\t</div>\r\n\t\t    <div class=\"halo-sidebar-wrapper\">\r\n\t\t        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\r\n\t\t            <div class=\"login-form-wrapper\">\r\n\t\t                <div class=\"form-field\">\r\n\t\t                    <label class=\"form-label\" for=\"login_email\">\r\n\t\t                        {{lang 'login.field_email'}}\r\n\t\t                    </label>\r\n\t\t                    <input id=\"login_email\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"\">\r\n\t\t                </div>\r\n\t\t                <div class=\"form-field\">\r\n\t\t                    <label class=\"form-label\" for=\"login_pass\">\r\n\t\t                        {{lang 'login.field_password'}}\r\n\t\t                    </label>\r\n\t\t                    <input id=\"login_pass\" class=\"form-input\" type=\"password\" value=\"\" name=\"login_pass\" placeholder=\"\">\r\n\t\t                </div>\r\n\t\t                <div class=\"form-actions\">\r\n\t\t                    <button type=\"button\" class=\"button button--primary\">{{lang 'login.submit_value'}}</button>\r\n\t\t                    <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">\r\n\t\t                    \t<span>{{lang 'login.forgot_password'}}</span>\r\n\t\t                    </a>\r\n\t\t                    <a class=\"create-account button button--secondary button--transparent-2\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\r\n\t\t                </div>\r\n\t\t            </div>\r\n\t\t        </form>\r\n\t\t    </div>\r\n\t\t</div>\r\n\t{{/if}}\r\n{{/if}}\r\n",
  "components/halothemes/sidebar/halo-cart-sidebar": "{{#if page_type '!=' 'cart'}}\r\n    <div class=\"halo-sidebar halo-cart-sidebar halo-sidebar-right\" id=\"halo-cart-sidebar\">\r\n    \t<div class=\"halo-sidebar-header\">\r\n    \t\t<span class=\"title\">{{lang 'cart.cart_preview'}}</span>\r\n    \t\t<a href=\"#\" class=\"close\">\r\n    \t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n                \t<title>{{lang 'common.close'}}</title>\r\n                \t<desc>{{lang 'common.close'}}</desc>\r\n                \t<use xlink:href=\"#icon-close\"></use>\r\n                </svg>\r\n    \t\t</a>\r\n    \t</div>\r\n        <div class=\"halo-sidebar-wrapper haloCustomScrollbar\">\r\n            {{> components/common/cart-preview}}\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n",
  "components/halothemes/sidebar/halo-menu-sidebar": "<div class=\"halo-sidebar halo-menu-sidebar halo-sidebar-left{{#if theme_settings.haloMegamenu}} halo-menu-megamenu{{/if}}\" id=\"halo-menu-sidebar\">\r\n    <a href=\"#\" class=\"halo-sidebar-close\">\r\n        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n        \t<title>{{lang 'common.close'}}</title>\r\n        \t<desc>{{lang 'common.close'}}</desc>\r\n        \t<use xlink:href=\"#icon-close\"></use>\r\n        </svg>\r\n    </a>\r\n    <div class=\"halo-sidebar-wrapper navPages-container\" data-menu>\r\n        {{> components/common/navigation-mobile }}\r\n    </div>\r\n</div>\r\n",
  "components/halothemes/sidebar/halo-search-sidebar": "<div class=\"halo-sidebar halo-search-sidebar halo-sidebar-left\" id=\"halo-search-sidebar\">\n    <div class=\"halo-sidebar-header\">\n\t\t<span class=\"title\">{{lang 'common.search'}}</span>\n\t\t<a href=\"#\" class=\"close\">\n\t\t\t<svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\n            \t<title>{{lang 'common.close'}}</title>\n            \t<desc>{{lang 'common.close'}}</desc>\n            \t<use xlink:href=\"#icon-close\"></use>\n            </svg>\n\t\t</a>\n\t</div>\n    <div class=\"halo-sidebar-wrapper\"></div>\n</div>\n",
  "components/halothemes/sidebar/halo-before-you-leave": "{{#if theme_settings.halobeforeyouleave}}\r\n    <div id=\"halo-leave-sidebar\" class=\"halo-sidebar halo-leave-sidebar halo-sidebar-right\" data-time=\"{{multiply theme_settings.halo-before-you-leave-time 60000}}\">\r\n        <a href=\"#\" class=\"halo-sidebar-close\">\r\n\t        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n            \t<title>{{lang 'common.close'}}</title>\r\n            \t<desc>{{lang 'common.close'}}</desc>\r\n            \t<use xlink:href=\"#icon-close\"></use>\r\n            </svg>\r\n\t    </a>\r\n        <div class=\"halo-sidebar-wrapper\">\r\n            {{#if theme_settings.halo-before-you-leave-logo}}\r\n\t\t\t\t<div class=\"before-you-leave-header\">\r\n\t\t\t\t\t<a href=\"/\">\r\n\t\t\t\t\t\t{{#if settings.store_logo.image}}\r\n\t\t\t            \t<img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\r\n\t\t\t            {{else}}\r\n\t\t\t            \t<span>{{settings.store_logo.title}}</span>\r\n\t\t\t            {{/if}}\r\n\t\t\t        </a>\r\n\t\t\t\t</div>\r\n\t\t\t{{/if}}\r\n\t\t\t<div class=\"before-you-leave-banner\">\r\n\t\t\t\t<div class=\"image{{#unless theme_settings.halo-before-you-leave-banner-img}} no-image{{/unless}}\">\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-banner-img}}\r\n\t\t\t\t\t\t<img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-before-you-leave-banner-img}}\" alt=\"{{theme_settings.halo-before-you-leave-title}}\" title=\"{{theme_settings.halo-before-you-leave-title}}\">\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"content\">\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-title}}\r\n\t\t\t\t\t\t<h3 class=\"title\">{{theme_settings.halo-before-you-leave-title}}</h3>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-text-1}}\r\n\t\t\t\t\t\t<p class=\"desc1\">{{theme_settings.halo-before-you-leave-text-1}}</p>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-subheading-1}}\r\n\t\t\t\t\t\t<h3 class=\"heading1\">\r\n\t\t\t\t\t\t\t<span class=\"text\">{{theme_settings.halo-before-you-leave-subheading-1}}</span>\r\n\t\t\t\t\t\t</h3>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-text-2}}\r\n\t\t\t\t\t\t<p class=\"desc2\">{{theme_settings.halo-before-you-leave-text-2}}</p>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-subheading-2}}\r\n\t\t\t\t\t\t<h3 class=\"heading2\">{{theme_settings.halo-before-you-leave-subheading-2}}</h3>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if theme_settings.halo-before-you-leave-banner-btn-text}}\r\n\t\t\t\t\t\t<a class=\"button button--primary\" href=\"{{theme_settings.halo-before-you-leave-banner-link}}\">\r\n\t\t\t\t\t\t\t<span class=\"text\">{{theme_settings.halo-before-you-leave-banner-btn-text}}</span>\r\n\t\t\t\t\t\t</a>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"before-you-leave-tab\">\r\n\t\t\t\t<div class=\"tabs-wrapper\">\r\n\t\t\t\t\t<ul class=\"tabs\" data-tab>\r\n\t\t\t            {{#if theme_settings.halo-before-you-leave-recommended-text}}\r\n\t\t\t                <li class=\"tab is-active recommended\" data-id=\"recommended\">\r\n\t\t\t                    <a class=\"tab-title\" href=\"#tab-recommended\">{{theme_settings.halo-before-you-leave-recommended-text}}<span class=\"count\">0</span></a>\r\n\t\t\t                </li>\r\n\t\t\t            {{/if}}\r\n\t\t\t            {{#if theme_settings.halo-before-you-leave-history-text}}\r\n\t\t\t                <li class=\"tab history{{#unless theme_settings.halo-before-you-leave-recommended-text}} is-active{{/unless}}\" data-id=\"history\">\r\n\t\t\t                \t<a class=\"tab-title\" href=\"#tab-history\">{{theme_settings.halo-before-you-leave-history-text}}<span class=\"count\">0</span></a>\r\n\t\t\t                </li>\r\n\t\t\t            {{/if}}\r\n\t\t\t        </ul>\r\n\t\t\t        {{#if theme_settings.halo-before-you-leave-search}}\r\n\t\t\t\t        <div class=\"before-you-leave-search\">\r\n\t\t\t\t        \t<a class=\"search-icon\" href=\"#\" data-search-leave>\r\n\t\t\t\t                <svg class=\"icon icon-search\" role=\"img\" aria-label=\"{{lang 'search.quick_search.input_label'}}\">\r\n\t\t\t\t                \t<title>{{lang 'search.quick_search.input_label'}}</title>\r\n                                \t<desc>{{lang 'search.quick_search.input_placeholder'}}</desc>\r\n\t\t\t\t                \t<use xlink:href=\"#icon-search\"></use>\r\n\t\t\t\t                </svg>\r\n\t\t\t\t                <svg class=\"icon icon-close\" role=\"img\" aria-label=\"{{lang 'common.close'}}\">\r\n\t\t\t\t                \t<title>{{lang 'common.close'}}</title>\r\n                                \t<desc>{{lang 'common.close'}}</desc>\r\n\t\t\t\t                \t<use xlink:href=\"#icon-close\"></use>\r\n\t\t\t\t                </svg>\r\n\t\t\t\t            </a>\r\n\t\t\t\t            {{> components/halothemes/sidebar/halo-before-you-leave-search}}\r\n\t\t\t\t        </div>\r\n\t\t\t        {{/if}}\r\n\t\t\t\t</div>\r\n\t\t\t    <div class=\"tabs-contents\">\r\n\t\t\t        {{#if theme_settings.halo-before-you-leave-recommended-text}}\r\n\t\t\t            <div class=\"tab-content is-active\" id=\"tab-recommended\">\r\n\t\t\t            \t<div id=\"recommended-products-list-tmp\" class=\"tab-content--product\">\r\n\t\t\t            \t\t<div class=\"product-slider halo-dots halo-arrows-custom\"></div>\r\n\t\t\t            \t</div>\r\n\t\t\t          </div>\r\n\t\t\t        {{/if}}\r\n\t\t\t        {{#if theme_settings.halo-before-you-leave-history-text}}\r\n\t\t\t            <div class=\"tab-content{{#unless theme_settings.halo-before-you-leave-recommended-text}} is-active{{/unless}}\" id=\"tab-history\">\r\n\t\t\t\t            <div id=\"history-products-list-tmp\" class=\"tab-content--product\">\r\n\t\t\t\t                <div class=\"no-products\">\r\n\t\t\t                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.no_item'}}\">\r\n\t\t\t                        \t<title>{{lang 'common.no_item'}}</title>\r\n                            \t\t\t<desc>{{lang 'common.no_item'}}</desc>\r\n\t\t\t                        \t<use xlink:href=\"#icon-no-products\"></use>\r\n\t\t\t                        </svg>\r\n\t\t\t                        <div class=\"text\">{{lang 'common.no_item'}}</div>\r\n\t\t\t                    </div>\r\n\t\t\t                    <div class=\"product-slider halo-dots halo-arrows-custom\"></div>\r\n\t\t\t\t            </div> \r\n\t\t\t            </div>\r\n\t\t\t        {{/if}}\r\n\t\t\t    </div>\r\n\t\t\t</div>\r\n\t\t\t{{#if theme_settings.halo-before-you-leave-back-text}}\r\n\t\t\t\t<div class=\"before-you-leave-back\">\r\n\t\t\t\t\t<a class=\"button\" href=\"/\">\r\n\t\t\t\t\t\t<span class=\"text\">{{theme_settings.halo-before-you-leave-back-text}}</span>\r\n\t\t\t\t\t</a>\r\n\t\t\t\t</div>\r\n\t\t\t{{/if}}\r\n        </div>\r\n    </div>\r\n{{/if}}\r\n",
  "components/common/cart-preview": "<div class=\"previewCart\">\r\n    {{#if cart.items.length}}\r\n        <div class=\"previewCartHeader\">\r\n            <span class=\"text\">\r\n                <span>{{lang 'cart.items' NUM=cart.quantity}}</span>\r\n            </span>\r\n        </div>\r\n    {{/if}}\r\n    {{#if cart.items.length}}\r\n        <ul class=\"previewCartList\" data-cart-quantity=\"{{cart.quantity}}\">\r\n            {{#each cart.items}}\r\n                <li class=\"previewCartItem\" {{#if options}}data-cart-edit-productItem{{/if}}>\r\n                    <div class=\"previewCartItem-image\">\r\n                        {{#if type '==' 'GiftCertificate'}}\r\n                            <img src=\"{{cdn ../../theme_settings.default_image_gift_certificate}}\" alt=\"GiftCertificate\" title=\"GiftCertificate\">\r\n                        {{else}}\r\n                            {{> components/common/responsive-img\r\n                                image=image\r\n                                fallback_size=../../theme_settings.productcartthumb_size\r\n                                lazyload=../../theme_settings.lazyload_mode\r\n                                default_image=../../theme_settings.default_image_product\r\n                            }}\r\n                        {{/if}}\r\n                    </div>\r\n                    <div class=\"previewCartItem-content\">\r\n                        <h6 class=\"previewCartItem-name\">\r\n                            <a href=\"{{url}}\" alt=\"{{name}}\" title=\"{{name}}\" style=\"-webkit-box-orient: vertical;\">{{name}}</a>\r\n                        </h6>\r\n                        {{#if options}}\r\n                            <div class=\"previewCartItem-options\">\r\n                                <span class=\"previewCartItem-variant\">\r\n                                    {{#each options}}\r\n                                        <span>{{{ sanitize value}}}</span>\r\n                                    {{/each}}\r\n                                </span>\r\n                                <a href=\"#\" class=\"previewCartItem-edit\"  data-cart-edit-id=\"{{product_id}}\" data-cart-itemid=\"{{id}}\" data-cart-edit-price=\"{{price.formatted}}\" data-cart-edit-quantity=\"{{quantity}}\">\r\n                                    <svg class=\"icon\"><use xlink:href=\"#icon-edit\"></use></svg>\r\n                                </a>\r\n                            </div>\r\n                        {{/if}}\r\n                        <span class=\"previewCartItem-price\">\r\n                            {{#or ../customer (if ../theme_settings.restrict_to_login '!==' true)}}\r\n                                <span{{#if price_discounted}} class=\"price--discounted\"{{/if}}>{{price.formatted}}</span>\r\n                                {{#if price_discounted}}\r\n                                    {{price_discounted.formatted}}\r\n                                {{/if}}\r\n                            {{else}}\r\n                                {{> components/common/login-for-pricing}}\r\n                            {{/or}}\r\n                        </span>\r\n                        <div class=\"previewCartItem-qty\">\r\n                            <label class=\"form-label\" for=\"qty-{{id}}\">{{lang 'products.quantity'}}</label>\r\n                            <div class=\"form-increment\">\r\n                                <input class=\"form-input form-input--incrementTotal\"\r\n                                    id=\"qty-{{id}}\"\r\n                                    name=\"qty-{{id}}\"\r\n                                    type=\"number\"\r\n                                    value=\"{{quantity}}\"\r\n                                    data-quantity-min=\"{{min_purchase_quantity}}\"\r\n                                    data-quantity-max=\"{{max_purchase_quantity}}\"\r\n                                    data-quantity-min-error=\"{{lang 'products.quantity_min' quantity=min_purchase_quantity}}\"\r\n                                    data-quantity-max-error=\"{{lang 'products.quantity_max' quantity=max_purchase_quantity}}\"\r\n                                    min=\"1\"\r\n                                    pattern=\"[0-9]*\"\r\n                                    data-cart-itemid=\"{{id}}\"\r\n                                    data-action=\"manualQtyChange\"\r\n                                    aria-live=\"polite\"{{#unless can_modify}} disabled{{/unless}}>\r\n                            </div>\r\n                        </div>\r\n                        {{#if can_modify}}\r\n                            <a class=\"previewCartItem-remove icon\" data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">\r\n                                <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.confirm_delete'}}\">\r\n                                    <title>{{lang 'cart.confirm_delete'}}</title>\r\n                                    <desc>{{lang 'cart.confirm_delete'}}</desc>\r\n                                    <use xlink:href=\"#icon-close\"></use>\r\n                                </svg>\r\n                            </a>\r\n                        {{else}}\r\n                            {{#if type '==' 'GiftCertificate'}}\r\n                                <a class=\"previewCartItem-remove icon\" data-cart-itemid=\"{{id}}\" href=\"#\" data-confirm-delete=\"{{lang 'cart.confirm_delete'}}\">\r\n                                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'cart.confirm_delete'}}\">\r\n                                        <title>{{lang 'cart.confirm_delete'}}</title>\r\n                                        <desc>{{lang 'cart.confirm_delete'}}</desc>\r\n                                        <use xlink:href=\"#icon-close\"></use>\r\n                                    </svg>\r\n                                </a>\r\n                            {{/if}}\r\n                        {{/if}}\r\n                    </div>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <ul class=\"previewCartTotalsPrice\">\r\n            <li class=\"previewCartTotals subTotal\">\r\n                <div class=\"previewCartTotals-label\">\r\n                    {{lang 'cart.checkout.subtotal'}}:\r\n                </div>\r\n                <div class=\"previewCartTotals-value\">\r\n                    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                        {{cart.sub_total.formatted}}\r\n                    {{else}}\r\n                        {{> components/common/login-for-pricing}}\r\n                    {{/or}}\r\n                </div>\r\n            </li>\r\n            <li class=\"previewCartTotals grandTotal\">\r\n                <div class=\"previewCartTotals-label\">\r\n                    {{lang 'cart.checkout.grand_total'}}:\r\n                </div>\r\n                <div class=\"previewCartTotals-value cart-total-grandTotal\">\r\n                    {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\r\n                        <span>{{cart.grand_total.formatted}}</span>\r\n                    {{else}}\r\n                        {{> components/common/login-for-pricing}}\r\n                    {{/or}}\r\n                </div>\r\n            </li>\r\n        </ul>\r\n        <div class=\"previewCartAction\">\r\n            {{#if cart.show_primary_checkout_button}}\r\n                <div class=\"previewCartAction-checkout\">\r\n                    <a href=\"{{urls.checkout.single_address}}\" class=\"button button--primary\">\r\n                        {{lang 'cart.preview.checkout_now'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            {{#if cart.show_multiple_address_shipping}}\r\n                <div class=\"previewCartAction-checkoutMultiple\">\r\n                    <a href=\"{{urls.checkout.multiple_address}}\">\r\n                        {{lang 'cart.preview.checkout_multiple'}}\r\n                    </a>\r\n                </div>\r\n            {{/if}}\r\n\r\n            <div class=\"previewCartAction-viewCart\">\r\n                <a href=\"{{urls.cart}}\" class=\"button button--secondary button--transparent-2\">\r\n                    {{lang 'cart.preview.view_cart'}}\r\n                </a>\r\n            </div>\r\n        </div>\r\n    {{else}}\r\n        <div class=\"previewCart-emptyBody\">\r\n            {{lang 'cart.checkout.empty_cart'}}\r\n        </div>\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/navigation-mobile": "<nav class=\"navPages\">\r\n    <ul class=\"navPages-list\"></ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            {{#if customer}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.account.index}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                            <title>{{lang 'common.account'}}</title>\r\n                            <desc>{{lang 'common.account'}}</desc>\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'account.title'}}\r\n                    </span>\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.auth.login}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                            <title>{{lang 'common.account'}}</title>\r\n                            <desc>{{lang 'common.account'}}</desc>\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'login.title'}}\r\n                    </span>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            <a class=\"navPages-action navPages-action-end\" href=\"/wishlist.php\">\r\n                <span class=\"text\">\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n                        <title>{{lang 'account.wishlists.heading'}}</title>\r\n                        <desc>{{lang 'account.wishlists.heading'}}</desc>\r\n                        <use xlink:href=\"#icon-heart\"></use>\r\n                    </svg>\r\n                    {{lang 'account.wishlists.heading'}}\r\n                </span>\r\n            </a>\r\n        </li>\r\n        {{#if settings.phone_number}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"tel:{{settings.phone_number}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.addresses.phone'}}\">\r\n                            <title>{{lang 'account.addresses.phone'}}</title>\r\n                            <desc>{{lang 'account.addresses.phone'}}</desc>\r\n                            <use xlink:href=\"#icon-phone\"></use>\r\n                        </svg>\r\n                        {{settings.phone_number}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.halo-book-an-appointment}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <a class=\"navPages-action navPages-action-end\" href=\"#\" data-book-an-appointment>\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{theme_settings.halo-book-an-appointment-title}}\">\r\n                            <title>{{theme_settings.halo-book-an-appointment-title}}</title>\r\n                            <desc>{{theme_settings.halo-book-an-appointment-title}}</desc>\r\n                            <use xlink:href=\"#icon-booking\"></use>\r\n                        </svg>\r\n                        {{theme_settings.halo-book-an-appointment-title}}\r\n                    </span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item navPages-item-user has-dropdown\">\r\n                <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\r\n                    <a class=\"text\" href=\"#\">\r\n                        <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \r\n                        {{currency_selector.active_currency_code}}\r\n                    </a>\r\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                    </span>\r\n                </p>\r\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\r\n                            <p class=\"navPage-subMenu-action navPages-action\">\r\n                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                    <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\r\n                                </span>\r\n                                <a class=\"text\" href=\"#\">\r\n                                    {{currency_selector.active_currency_code}}\r\n                                </a>\r\n                            </p>\r\n                        </li>\r\n                        {{#each currency_selector.currencies}}\r\n                            <li class=\"navPage-subMenu-item-child navPages-action-end\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\r\n                                    <span class=\"text\"><span class=\"flag\">{{{flag}}}</span>{{name}}</span>\r\n                                </a>\r\n                            </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.show_social-links}}\r\n            <li class=\"navPages-item navPages-item-user\">\r\n                <div class=\"navPages-action navPages-action-end\">\r\n                    <div class=\"text\">\r\n                        {{> components/common/social-links position=\"newsletter\"}}\r\n                    </div>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "components/halothemes/sidebar/halo-before-you-leave-search": "<form class=\"form\" action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"is-srOnly\" for=\"search_query2\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"form-input\" data-search-quick name=\"search_query2\" id=\"search_query2\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button type=\"submit\"><svg class=\"icon\"><use xlink:href=\"#icon-search2\"></use></svg></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\n",
  "components/common/responsive-img": "{{!--\nThis file implements the lazysizes srcset pattern (as seen on\nhttps://github.com/aFarkas/lazysizes#modern-transparent-srcset-pattern) for maximum performance on modern browsers\nand reasonable compatibility with legacy browsers.\n\nThere are a few important arguments to know about when using this component:\n`lazyload`:\n    - If set to 'lazyload+lqip', will load a low-res image which will be replaced by lazysizes - ensuring that SOME\n    content is shown immediately on page load. Without LQIP, a transparent gif will be used instead, which reduces\n    the number of requests needed to load the page, making it even faster.\n    - If set to 'lazyload', the browser will not load the full-size image at first, instead the lazysizes plugin will\n        load it progressively after page load. This improves the load speed of the page, but it should be disabled for\n        high-priority above-the-fold images.\n    - If set to 'disabled', will not attempt to lazyload the image and instead tell the browser to load it immediately.\n        Use this for high-priority images that are above the fold.\n\n`fallback_size`:\n    This is the size to use for this image in legacy browsers that do not support srcset.\n    Can be defined as a pixel bounding-box size (e.g. \"123x123) or inherent width (e.g. \"123w\").\n\n`class`:\n    The CSS classes to add to the image, e.g. \"card-image\". If lazyloading is enabled, all images will have the\n    \"lazyload\" class so that lazysizes will target them automatically.\n\n`otherAttributes`:\n    Any other HTML attributes you want on the img tag, for example \"height='100' width='100'\"\n\n`default_image`:\n    The default image to use if `image` is undefined or falsy. Should be a theme asset, usually defined as a relative\n    path in the theme config. If a default image is not provided, you'll get an image tag with no image if the primary\n    image is undefined.\n\n`lqip_size`:\n    If you want to specify a particular size for the LQIP image, you can do so with this argument. A default of 80\n    pixels wide will be used otherwise. This argument has no effect if the lazyload mode is not \"lazyload+lqip\".\n\n--}}\n<img {{#if image}}{{!-- Get the default image for legacy browsers that do not support srcset\n--}}src=\"{{getImageSrcset image 1x=(default fallback_size '160w')}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\" {{!--\n  Allow lazysizes to generate the 'sizes' attribute automatically --}}data-sizes=\"auto\"\n  {{#unless lazyload '==' 'disabled'}}\n    {{#if lazyload '==' 'lazyload+lqip'}}\n    {{!-- If we want a LQIP, include it as the default srcset (will be overwritten by lazysizes) --}}\n    srcset=\"{{getImageSrcset image 1x=(default lqip_size '80w')}}\"\n    {{else}}\n      {{!-- If we don't want a LQIP, use a 1px transparent gif for the stand-in image\n            This requires no additional network request, so it's fast --}}\n    srcset=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\"\n    {{/if}}\n{{/unless}}\n{{!-- If lazyloading is disabled, just use \"srcset\". If it is not disabled, use \"data-srcset\" so lazysizes will use\n      this after page load --}}\n{{!--\nGenerate the srcset string using the default sizes defined in the helper. This will allow the browser to\npick the best-sized image based on its own understanding of how it will render the page.\n--}}\n{{#unless lazyload '==' 'disabled'}}data-{{/unless}}srcset=\"{{getImageSrcset image use_default_sizes=true }}\"\n{{else if default_image}}src=\"{{cdn default_image}}\"{{/if}}\nclass=\"{{#unless lazyload '==' 'disabled'}}lazyload{{/unless}}{{#if class}} {{class}}{{/if}}\"\n{{#if lazyload '==' 'lazyload'}}loading=\"lazy\"{{/if}}\n{{otherAttributes}} />\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/common/social-links": "<ul class=\"socialLinks socialLinks--alt\">\n\t{{#if position '==' 'footer'}}\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{display_name}}\">\n                        <title>{{display_name}}</title>\n                        <desc>{{display_name}}</desc>\n                        <use xlink:href=\"#icon-{{display_name}}\" />\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{theme_settings.show_social-links-messenger}}\">\n                        <title>{{theme_settings.show_social-links-messenger}}</title>\n                        <desc>{{theme_settings.show_social-links-messenger}}</desc>\n                        <use xlink:href=\"#icon-messenger\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{theme_settings.show_social-links-snapchat}}\">\n                        <title>{{theme_settings.show_social-links-snapchat}}</title>\n                        <desc>{{theme_settings.show_social-links-snapchat}}</desc>\n                        <use xlink:href=\"#icon-snapchat\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n</ul>\n"
}