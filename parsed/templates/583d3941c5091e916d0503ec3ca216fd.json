{
  "components/products/description-tabs": "<ul class=\"tabs\" data-tab>\n    <li class=\"tab is-active\">\n        <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\n    </li>\n    {{#all settings.show_product_reviews theme_settings.show_product_reviews theme_settings.show_product_reviews_tabs}}\n        <li class=\"tab productView-reviewTabLink\">\n            <a class=\"tab-title productView-reviewTabLink\" href=\"#tab-reviews\">{{lang 'products.reviews.header'}}</a>\n        </li>\n    {{/all}}\n    {{#all product.warranty theme_settings.show_warranty_tabs}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\n        </li>\n    {{/all}}\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-custom-fields\">{{lang 'products.custom_fields'}}</a>\n        </li>\n    {{/all}}\n    {{#if theme_settings.show_shipping_tab}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-shipping\">{{lang 'products.shipping_tab'}}</a>\n        </li>\n    {{/if}}\n    {{#if theme_settings.show_custom_tab}}\n        <li class=\"tab\">\n            <a class=\"tab-title\" href=\"#tab-custom\">{{lang 'products.custom_tab'}}</a>\n        </li>\n    {{/if}}\n</ul>\n<div class=\"tabs-contents\">\n    <div class=\"tab-content is-active\" id=\"tab-description\">\n        <div class=\"toggle-title\">\n            <a class=\"toggleLink is-open\" data-collapsible href=\"#tab-description-mobile\">\n                <span class=\"text\">{{lang 'products.description'}}</span>\n                <span class=\"icon-plus\">&nbsp;</span>\n            </a>\n        </div>\n        <div class=\"toggle-content is-open\" id=\"tab-description-mobile\">\n            {{{product.description}}}\n\n            {{#if product.description.length '>' 200}}\n                <div id=\"tab-descriptionShowmore\" class=\"tab-descriptionShowmore show\">\n                    <a class=\"button button--transparent-2 button--secondary\" href=\"#\" data-show-more-text=\"{{lang 'products.read_more'}}\" data-show-less-text=\"{{lang 'products.read_less'}}\"> \n                        {{lang 'products.read_more'}}\n                    </a>\n                </div>\n            {{/if}}\n        </div>\n    </div>\n    {{#all settings.show_product_reviews theme_settings.show_product_reviews theme_settings.show_product_reviews_tabs}}\n        <div class=\"tab-content\" id=\"tab-reviews\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-reviews-mobile\">\n                    <span class=\"text\">{{lang 'products.reviews.header'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-reviews-mobile\">\n                {{> components/products/reviews reviews=product.reviews product=product urls=urls}}\n            </div>\n        </div>\n    {{/all}}\n    {{#all product.warranty theme_settings.show_warranty_tabs}}\n        <div class=\"tab-content\" id=\"tab-warranty\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-warranty-mobile\">\n                    <span class=\"text\">{{lang 'products.warranty'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-warranty-mobile\">\n                <div class=\"warranty-text\">{{{product.warranty}}}</div>\n            </div>\n        </div>\n    {{/all}}\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\n        <div class=\"tab-content\" id=\"tab-custom-fields\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-custom-fields-mobile\">\n                    <span class=\"text\">{{lang 'products.custom_fields'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-custom-fields-mobile\">\n                <div class=\"scrollable-wrapper\">\n                    <table>\n                        <tbody>\n                            {{#each product.custom_fields}}\n                                {{#startsWith '__' name}}\n                                {{else}}\n                                    <tr>\n                                        <td class=\"tb-title\">{{name}}</td>\n                                        <td>{{value}}</td>\n                                    </tr>\n                                {{/startsWith}}\n                            {{/each}}\n                        </tbody>\n                    </table>\n                </div>\n            </div>\n        </div>\n    {{/all}}\n    {{#if theme_settings.show_shipping_tab}}\n        <div class=\"tab-content\" id=\"tab-shipping\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-shipping-mobile\">\n                    <span class=\"text\">{{lang 'products.shipping_tab'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-shipping-mobile\"></div>\n        </div>\n    {{/if}}\n    {{#if theme_settings.show_custom_tab}}\n        <div class=\"tab-content\" id=\"tab-custom\">\n            <div class=\"toggle-title\">\n                <a class=\"toggleLink\" data-collapsible href=\"#tab-custom-mobile\">\n                    <span class=\"text\">{{lang 'products.custom_tab'}}</span>\n                    <span class=\"icon-plus\">&nbsp;</span>\n                </a>\n            </div>\n            <div class=\"toggle-content\" id=\"tab-custom-mobile\"></div>\n        </div>\n    {{/if}}\n</div>\n",
  "components/products/reviews": "<div class=\"productView-reviews-wrapper\">\n    {{#if reviews.total}}\n        <div class=\"productReview-total\">\n            <span>\n                {{lang 'products.reviews.link_to_review' total=reviews.total}}\n            </span>\n        </div>\n        <ul class=\"productReviews-list\" id=\"productReviews-list\">\n            {{#each reviews.list}}\n                <li class=\"productReview\">\n                    <article itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\">\n                        <header>\n                            <h5 itemprop=\"name\" class=\"productReview-title\">\n                                <span class=\"productReview-title-text\">{{{ sanitize title }}}</span>\n                                <span class=\"productReview-rating rating--small\" itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\n                                    {{> components/products/ratings rating=rating}}\n                                    <span class=\"productReview-ratingNumber\" itemprop=\"ratingValue\">{{ rating }}</span>\n                                </span>\n                            </h5>\n                            {{#if name}}\n                                <meta itemprop=\"author\" content=\"{{name}}\">\n                                <p class=\"productReview-author\">\n                                    {{lang 'products.reviews.post_on_by' date=date name=name }}\n                                </p>\n                            {{else}}\n                                <p class=\"productReview-author\">\n                                    {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\n                                </p>\n                            {{/if}}\n                        </header>\n                        <p itemprop=\"reviewBody\" class=\"productReview-body\">{{{ sanitize text }}}</p>\n                    </article>\n                </li>\n            {{/each}}\n        </ul>\n        {{#or pagination.reviews.next pagination.reviews.previous}}\n            {{> components/common/paginator pagination.reviews}}\n        {{/or}}\n        <div class=\"button-createReview\">\n            <a class=\"button button--secondary button--transparent-2\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n        </div>\n    {{else}}\n        <div class=\"alertBox alertBox--warning\">\n            <div class=\"alertBox-column alertBox-icon\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-info\"></use></svg>\n            </div>\n            <p class=\"alertBox-column alertBox-message\">\n                {{lang 'products.reviews.write_a_review_text'}}\n            </p>\n        </div>\n        <div class=\"button-createReview\">\n            <a class=\"button button--secondary button--transparent-2\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n        </div>\n    {{/if}}\n</div>\n",
  "components/products/ratings": "{{#for 1 5}}\n    {{#if ../this.rating '>=' $index}}\n        <span class=\"icon icon--ratingFull\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{else}}\n        <span class=\"icon icon--ratingEmpty\">\n            <svg>\n                <use xlink:href=\"#icon-star\" />\n            </svg>\n        </span>\n    {{/if}}\n{{/for}}\n",
  "components/common/paginator": "<nav class=\"pagination{{#if theme_settings.halo-product-show-more}} pagination-2{{/if}}\" aria-label=\"pagination\">\n    {{#if page_type '==' 'category'}}\n        {{#if total}}\n            {{#if start}}\n                {{#if end}}\n                    {{#if end '<' total}}\n                        <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{end}}</span> of <span class=\"total\">{{total}}</span> total</div>\n                    {{else}}\n                        <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{total}}</span> of <span class=\"total\">{{total}}</span> total</div>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--secondary button--transparent-2\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--secondary button--transparent-2 disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'brand'}}\n        {{#if links.length}}\n            {{#each shop_by_brand}}\n                {{#if name '==' ../brand.name}}\n                    <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{count}}</span> total</div>\n                {{/if}}\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--secondary button--transparent-2\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--secondary button--transparent-2 disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'search'}}\n        <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{total}}</span> total</div>\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--secondary button--transparent-2\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--secondary button--transparent-2 disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <svg class=\"icon\">\n                        <use xlink:href=\"#icon-chevron-left\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}aria-current=\"page\"{{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <svg class=\"icon\">\n                        <use xlink:href=\"#icon-chevron-right\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}