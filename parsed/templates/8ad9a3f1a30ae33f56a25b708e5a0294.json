{
  "components/common/footer": "<footer class=\"footer\">\n    {{#and (if settings.show_newsletter_box) (if theme_settings.halo_subscription_form)}}\n        <div class=\"footer-subscription\">\n            <div class=\"container\">\n                {{#if theme_settings.halo_subscription_form-logo}}\n                    <div class=\"footer-subscription-logo\">\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo_subscription_form-logo}}\" alt=\"{{settings.newsletter_summary}}\" title=\"{{settings.newsletter_summary}}\">\n                    </div>\n                {{/if}}\n                <div class=\"footer-info-wrapper\"> \n                    {{#if settings.show_newsletter_summary }}\n                        <div class=\"footer-info-text\">\n                            <p>{{settings.newsletter_summary}}</p>\n                        </div>\n                    {{/if}}\n                    {{> components/common/subscription-form}}\n                </div>\n            </div>\n        </div>\n    {{/and}}\n    <div class=\"footer-top\" role=\"contentinfo\">\n        <div class=\"container\">\n            <div class=\"footer-top-wrapper\">\n                {{#if theme_settings.footer-info-1-heading}}\n                    <div class=\"footer-info-col footer-info-col--mobile footer-shop\" data-section-type=\"footer-categories\">\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer-info-1-heading}}</h3>\n                        <div class=\"footer-info-wrapper\"> \n                            <ul class=\"footer-info-list\">\n                                {{#each categories}}\n                                    <li>\n                                        <a href=\"{{url}}\" class=\"footer-info-list-link\">\n                                            <span>{{name}}</span>\n                                        </a>\n                                    </li>\n                                {{/each}}\n                            </ul>\n                        </div>\n                    </div>\n                {{/if}}\n                {{#if theme_settings.footer-info-2-heading}}\n                    <div class=\"footer-info-col footer-info-col--mobile footer-about\" data-section-type=\"footer-webPages\">\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer-info-2-heading}}</h3>\n                        <div class=\"footer-info-wrapper\"> \n                            <ul class=\"footer-info-list\">\n                                {{#if theme_settings.footer-info-2-text1}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-2-link1}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-2-text1}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-2-text2}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-2-link2}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-2-text2}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-2-text3}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-2-link3}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-2-text3}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-2-text4}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-2-link4}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-2-text4}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-2-text5}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-2-link5}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-2-text5}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                            </ul>\n                        </div>\n                    </div>\n                {{/if}}\n                {{#if theme_settings.footer-info-3-heading}}\n                    <div class=\"footer-info-col footer-info-col--mobile footer-page-list\" data-section-type=\"footer-webPages\">\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer-info-3-heading}}</h3>\n                        <div class=\"footer-info-wrapper\"> \n                            <ul class=\"footer-info-list\">\n                                {{#if theme_settings.footer-info-3-text1}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-3-link1}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-3-text1}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-3-text2}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-3-link2}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-3-text2}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-3-text3}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-3-link3}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-3-text3}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-3-text4}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-3-link4}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-3-text4}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-3-text5}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-3-link5}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-3-text5}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                            </ul>\n                        </div>\n                    </div>\n                {{/if}}\n                {{#if theme_settings.footer-info-4-heading}}\n                    <div class=\"footer-info-col footer-info-col--mobile footer-service\" data-section-type=\"footer-webPages\">\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer-info-4-heading}}</h3>\n                        <div class=\"footer-info-wrapper\">\n                            <ul class=\"footer-info-list\">\n                                {{#if theme_settings.footer-info-4-text1}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-4-link1}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-4-text1}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-4-text2}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-4-link2}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-4-text2}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-4-text3}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-4-link3}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-4-text3}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-4-text4}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-4-link4}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-4-text4}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                                {{#if theme_settings.footer-info-4-text5}}\n                                    <li>\n                                        <a href=\"{{theme_settings.footer-info-4-link5}}\" class=\"footer-info-list-link\">\n                                            <span>{{theme_settings.footer-info-4-text5}}</span>\n                                        </a>\n                                    </li>\n                                {{/if}}\n                            </ul> \n                        </div>\n                    </div>\n                {{/if}}\n                {{#or (if settings.address) (if theme_settings.geotrust_ssl_common_name) (if theme_settings.footer-info-5-heading) (if settings.paypal_commerce_credit_message)}}\n                    <div class=\"footer-info-col footer-info\" data-section-type=\"storeInfo\">\n                        <h3 class=\"footer-info-heading\">{{theme_settings.footer-info-5-heading}}</h3>\n                        <div class=\"footer-info-wrapper\">\n                            <ul class=\"footer-info-list footer-info-list-2\">\n                                {{#all (if settings.phone_number) (if theme_settings.footer-info-5-phone)}}\n                                    <li>\n                                        <span class=\"footer-info-icon\">{{lang 'footer.call_us'}}</span>\n                                        <a href=\"tel:{{settings.phone_number}}\" class=\"footer-info-list-link\">\n                                            <span>{{settings.phone_number}}</span>\n                                        </a>\n                                    </li>\n                                {{/all}}\n                                {{#if theme_settings.footer-info-5-hour}}\n                                    <li>\n                                        <span class=\"footer-info-icon\">{{lang 'footer.hour'}}</span>\n                                        <span class=\"footer-info-text\">\n                                            {{theme_settings.footer-info-5-hour-1}}\n                                            <br/>\n                                            {{theme_settings.footer-info-5-hour-2}}\n                                        </span>\n                                    </li>\n                                {{/if}}\n                                {{#all (if settings.address) (if theme_settings.footer-info-5-address)}}\n                                    <li>\n                                        <span class=\"footer-info-icon\">{{lang 'footer.address'}}</span>\n                                        <address class=\"footer-info-text\">{{nl2br settings.address}}</address>\n                                    </li>\n                                {{/all}}\n                                {{#if settings.paypal_commerce_credit_message}}\n                                    <li>\n                                        <span class=\"footer-info-icon\">{{lang 'footer.paypal'}}</span>\n                                        <span class=\"footer-info-text paypal-credit\">{{nl2br settings.paypal_commerce_credit_message}}</span>\n                                    </li>\n                                {{/if}}\n                            </ul> \n                        </div>\n                        {{#if theme_settings.geotrust_ssl_common_name}}\n                            <div class=\"footer-geotrust-ssl-seal\">\n                                {{> components/common/geotrust-ssl-seal}}\n                            </div>\n                        {{/if}}\n                    </div>\n                {{/or}}\n            </div>\n        </div>\n    </div>\n    <div class=\"footer-bottom\">\n        <div class=\"container\">\n            <div class=\"footer-bottom-wrapper\">\n                {{#all (if theme_settings.show_social-links) (if social_media)}}\n                    <div class=\"footer-bottom-item footer-social-media text-left\">\n                        <div class=\"footer-bottom-item__label\">\n                            <span>{{lang 'social.connect'}}</span>\n                        </div>\n                        {{> components/common/social-links position=\"footer\"}}\n                    </div>\n                {{/all}}\n                {{#or (if theme_settings.show_copyright_footer) (if theme_settings.show_site_info_footer)}}\n                    <div class=\"footer-bottom-item text-center\">\n                        {{#if theme_settings.show_copyright_footer}}\n                            <div class=\"footer-copyright\">\n                                <p>&copy; {{moment format=\"YYYY\"}} {{settings.store_name}}.{{#if theme_settings.show_powered_by}} {{theme_settings.powered_by-text1}} <a href=\"{{theme_settings.powered_by-link}}\" rel=\"nofollow\"><span>{{theme_settings.powered_by-text2}}</span></a>{{/if}}</p>\n                            </div>\n                        {{/if}}\n                        {{#if theme_settings.show_site_info_footer}}\n                            <div class=\"footer-copyright footer-site-info\">\n                                <p>{{theme_settings.site_info-text1}} <a href=\"{{theme_settings.site_info-link}}\" class=\"link\"><span>{{theme_settings.site_info-text2}}</span></a></p>\n                            </div>\n                        {{/if}}\n                    </div>\n                {{/or}}\n                {{#if theme_settings.show_payments_footer}}\n                    <div class=\"footer-bottom-item footer-payment text-right\">\n                        {{> components/common/payment-icons}}\n                    </div>\n                {{/if}}\n            </div>\n        </div>\n    </div>\n</footer>\n{{#unless theme_settings.halo_recently_viewed_products }}\n    {{#if theme_settings.halo-back-to-top}}\n        <div id=\"haloBackToTop\" class=\"halo-back-to-top\">\n            <a href=\"#\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-keyboard-arrow-down\"></use></svg>\n            </a>\n        </div>\n    {{/if }}\n{{/unless}}\n",
  "components/common/subscription-form": "<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\"\n                    id=\"nl_email\"\n                    name=\"nl_email\"\n                    type=\"email\"\n                    value=\"{{customer.email}}\"\n                    placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n                    aria-label=\"{{lang 'newsletter.email_placeholder'}}\"\n                >\n                <button \n                    class=\"button button--primary button--icon form-prefixPostfix-button--postfix\"\n                    type=\"submit\"\n                >\n                    <span class=\"text\">{{lang 'newsletter.subscribe_submit'}}</span>\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'newsletter.subscribe_submit'}}\">\n                        <title>{{lang 'newsletter.subscribe_submit'}}</title>\n                        <desc>{{lang 'newsletter.subscribe_submit'}}</desc>\n                        <use xlink:href=\"#icon-long-arrow-right\"></use>\n                    </svg>\n                </button>\n            </div>\n        </div>\n    </fieldset>\n</form>\n",
  "components/common/social-links": "<ul class=\"socialLinks socialLinks--alt\">\n\t{{#if position '==' 'footer'}}\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{display_name}}\">\n                        <title>{{display_name}}</title>\n                        <desc>{{display_name}}</desc>\n                        <use xlink:href=\"#icon-{{display_name}}\" />\n                    </svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{theme_settings.show_social-links-messenger}}\">\n                        <title>{{theme_settings.show_social-links-messenger}}</title>\n                        <desc>{{theme_settings.show_social-links-messenger}}</desc>\n                        <use xlink:href=\"#icon-messenger\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg role=\"img\" aria-label=\"{{theme_settings.show_social-links-snapchat}}\">\n                        <title>{{theme_settings.show_social-links-snapchat}}</title>\n                        <desc>{{theme_settings.show_social-links-snapchat}}</desc>\n                        <use xlink:href=\"#icon-snapchat\" />\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n</ul>\n",
  "components/common/geotrust-ssl-seal": "<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n",
  "components/common/payment-icons": "{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_visa}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Visa\">\n                <title>Visa</title>\n                <desc>Visa</desc>\n                <use xlink:href=\"#icon-logo-visa\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Mastercard\">\n                <title>Mastercard</title>\n                <desc>Mastercard</desc>\n                <use xlink:href=\"#icon-logo-mastercard\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_amex}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Amex\">\n                <title>Amex</title>\n                <desc>Amex</desc>\n                <use xlink:href=\"#icon-logo-american-express\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Discover\">\n                <title>Discover</title>\n                <desc>Discover</desc>\n                <use xlink:href=\"#icon-logo-discover\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Paypal\">\n                <title>Paypal</title>\n                <desc>Paypal</desc>\n                <use xlink:href=\"#icon-logo-paypal\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Amazonpay\">\n                <title>Amazonpay</title>\n                <desc>Amazonpay</desc>\n                <use xlink:href=\"#icon-logo-amazonpay\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Google Pay\">\n                <title>Google Pay</title>\n                <desc>Google Pay</desc>\n                <use xlink:href=\"#icon-logo-googlepay\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_klarna}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Klarna\">\n                <title>Klarna</title>\n                <desc>Klarna</desc>\n                <use xlink:href=\"#icon-logo-klarna\"></use>\n            </svg>\n        {{/if}}\n        {{#if show_accept_estern_union}}\n            <svg class=\"footer-payment-icon\" role=\"img\" aria-label=\"Estern Union\">\n                <title>Estern Union</title>\n                <desc>Estern Union</desc>\n                <use xlink:href=\"#icon-logo-estern-union\"></use>\n            </svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n"
}