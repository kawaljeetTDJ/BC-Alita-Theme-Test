{
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}\" {{#if theme_settings.haloMenuResize}} id=\"halo-menu-resize\"{{/if}}>\n        {{#each categories}}\n            <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                {{> components/common/navigation-list-alternate}}\n            </li>\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                {{#if name '!=' 'Blog'}}\n                    <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                        {{> components/common/navigation-list-alternate}}\n                    </li>\n                {{/if}}\n            {{/each}}\n        {{/unless}}\n        {{#if theme_settings.haloMenuResize}}\n            <li class=\"navPages-item navPages-item-toggle u-hiddenVisually\">\n                <p class=\"navPages-action has-subMenu is-root\">\n                    <span class=\"text\">\n                        <svg><use xlink:href=\"#icon-more\"></use></svg>\n                    </span>\n                </p>\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n                    <ul class=\"navPage-subMenu-list\" id=\"halo-navPages-dropdown\" data-dropdown-menu-content></ul>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <p class=\"navPages-action has-subMenu is-root\" data-label=\"{{url}}\">\n        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action navPages-action-end\" data-label=\"{{url}}\" href=\"{{url}}\">\n      <span class=\"text\">{{name}}</span>\n    </a>\n{{/if}}\n\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n            </p>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n            <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child has-dropdown\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu\">\n                        <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span>\n                    </p>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal\" aria-hidden=\"true\" tabindex=\"-1\">\n                        <ul class=\"navPage-subMenu-list\">\n                            <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                                <p class=\"navPage-subMenu-action navPages-action\">\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                                    </span>\n                                    <a class=\"text\" href=\"{{url}}\">{{name}}</a>\n                                </p>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}