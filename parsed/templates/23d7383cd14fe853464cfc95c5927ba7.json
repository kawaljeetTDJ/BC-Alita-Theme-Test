{
  "components/common/navigation-mobile": "<nav class=\"navPages\">\r\n    <ul class=\"navPages-list\"></ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            {{#if customer}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.account.index}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                            <title>{{lang 'common.account'}}</title>\r\n                            <desc>{{lang 'common.account'}}</desc>\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'account.title'}}\r\n                    </span>\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.auth.login}}\">\r\n                    <span class=\"text\">\r\n                        <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'common.account'}}\">\r\n                            <title>{{lang 'common.account'}}</title>\r\n                            <desc>{{lang 'common.account'}}</desc>\r\n                            <use xlink:href=\"#icon-user\"></use>\r\n                        </svg>\r\n                        {{lang 'login.title'}}\r\n                    </span>\r\n                </a>\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-user\">\r\n            <a class=\"navPages-action navPages-action-end\" href=\"/wishlist.php\">\r\n                <span class=\"text\">\r\n                    <svg class=\"icon\" role=\"img\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\r\n                        <title>{{lang 'account.wishlists.heading'}}</title>\r\n                        <desc>{{lang 'account.wishlists.heading'}}</desc>\r\n                        <use x